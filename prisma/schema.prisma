// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id
  path String
  files File[]
}

model File {
  id Int @id
  filename String
  user User @relation(fields: [ownerId], references: [id])
  ownerId Int 
}